{
  "name": "WPshop",
  "version": "2.0.0",
  "description": "Submodule et GULP",
  "main": "gulpfile.js",
  "scripts": {
    "install": "git submodule init && git submodule update --remote --recursive --force",
    "update": "npm install",
    "start": "node node_modules/gulp/bin/gulp.js",
    "block-develop": "wp-scripts start --source-path=blocks/src/ --output-path=blocks/build/",
    "block-build": "wp-scripts build --webpack-src-dir=blocks/src/ --output-path=blocks/build/",
    "create-block": "npx @wordpress/create-block %npm_config_name% --target-dir=blocks/src/%npm_config_directory% --no-plugin --namespace wpshop --category wpshop"
  },
  "author": "Eoxia team",
  "license": "AGPLv3",
  "devDependencies": {
    "gulp": "latest",
    "gulp-autoprefixer": "latest",
    "gulp-concat": "latest",
    "gulp-rename": "latest",
    "gulp-sass": "latest",
    "gulp-uglify": "latest",
    "gulp-watch": "latest",
    "sass": "^1.87.0"
  },
  "dependencies": {
    "@wordpress/scripts": "^30.14.1",
    "natives": "latest"
  }
}
