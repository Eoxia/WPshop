{
  "name": "WPshop",
  "version": "2.0.0",
  "description": "Submodule et GULP",
  "main": "gulpfile.js",
  "scripts": {
    "install": "git submodule init && git submodule update --remote --recursive --force",
    "update": "npm install",
    "start": "node node_modules/gulp/bin/gulp.js",
    "blockstart": "wp-scripts start --webpack-src-dir=blocks/src/ --output-path=blocks/build/"
  },
  "author": "Eoxia team",
  "license": "AGPLv3",
  "devDependencies": {
    "gulp": "latest",
    "gulp-autoprefixer": "latest",
    "gulp-concat": "latest",
    "gulp-rename": "latest",
    "gulp-sass": "latest",
    "gulp-uglify": "latest",
    "gulp-watch": "latest"
  },
  "dependencies": {
    "@wordpress/scripts": "^30.14.1",
    "natives": "latest"
  }
}
